project Decision2 is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("decision_tree2.adb");
   for Exec_Dir use ".";

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-gnata", "-gnatwkmru", "-gnatyclux");
   end Compiler;

   package Builder is
      for Switches ("ada") use ("-s", "-g");
   end Builder;

   package Linker is
      for Switches ("ada") use ("-g");
   end Linker;

end Decision2;

